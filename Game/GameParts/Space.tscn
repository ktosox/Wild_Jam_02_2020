	col += texture(TEXTURE, UV-vec2(range.x,-range.y)*lod);
	col += texture(TEXTURE, UV+vec2(range.x,-range.y)*lod);
	col += texture(TEXTURE, UV-vec2(-range.x,range.y)*lod);
	col += texture(TEXTURE, UV+vec2(-range.x,range.y)*lod);
	col = col/7.0;
	COLOR = col;
}

